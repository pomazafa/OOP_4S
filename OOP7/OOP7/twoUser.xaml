<UserControl x:Class="OOP7.twoUser"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OOP7"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Button Grid.Row="0" Background="Brown" Click="Button_Click" IsHitTestVisible="True"/>
        <Button Grid.Row="1" Background="Blue" Click="Button_Click"/>
        <Button Grid.Column="2" Background="White" Click="Button_Click" Focusable="False"/>
        <Button Grid.Row="1" Grid.Column="1" Background="Yellow" Click="Button_Click"/>
        <Button Grid.Row="0" Grid.Column="1" Background="Fuchsia" Click="Button_Click" Focusable="False" IsHitTestVisible="True" IsTabStop="False"/>
        <Button Grid.Row="1" Grid.Column="2" Background="SeaGreen" Click="Button_Click"/>
    </Grid>
</UserControl>
